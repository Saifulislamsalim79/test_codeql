<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/intercom_reaction_shadow"/>
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/intercom_white"/>
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp"/>
            <stroke android:width="1px" android:color="@color/intercom_container_border"/>
        </shape>
    </item>
</layer-list>
